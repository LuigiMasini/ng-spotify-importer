{
    "name": "ng-slacker-to-spotify",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve & echo $! > .ng-serve.pid",
        "stop": "kill $(cat .ng-serve.pid)",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "wdio run e2e/wdio.conf.ts --autoCompileOpts.tsNodeOpts.project=e2e/tsconfig.json",
        "e2e:unit": "TS_NODE_PROJECT=e2e/tsconfig.json mocha -r ts-node/register e2e/**/*.spec.ts",
        "e2e:debug": "DEBUG=true npm run e2e"
    },
    "browser": {
        "crypto": false
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.9",
        "@angular/cdk": "^9.2.4",
        "@angular/common": "~9.1.9",
        "@angular/compiler": "~9.1.9",
        "@angular/core": "~9.1.9",
        "@angular/forms": "~9.1.9",
        "@angular/material": "^9.2.4",
        "@angular/platform-browser": "~9.1.9",
        "@angular/platform-browser-dynamic": "~9.1.9",
        "@angular/router": "~9.1.9",
        "crypto-js": "^4.0.0",
        "fast-xml-parser": "^3.17.1",
        "he": "^1.2.0",
        "papaparse": "^5.2.0",
        "rxjs": "~6.5.4",
        "spotify-web-api-js": "^1.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.7",
        "@angular/cli": "~9.1.7",
        "@angular/compiler-cli": "~9.1.9",
        "@types/he": "^1.1.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@types/papaparse": "^5.0.4",
        "@types/spotify-web-api-node": "^5.0.6",
        "@wdio/cli": "^7.16.11",
        "@wdio/local-runner": "^7.16.11",
        "@wdio/mocha-framework": "^7.16.11",
        "@wdio/spec-reporter": "^7.16.11",
        "@xmldom/xmldom": "^0.8.0",
        "chromedriver": "^90.0.0",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "mocha": "^9.1.3",
        "protractor": "~7.0.0",
        "spotify-web-api-node": "^5.0.2",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3",
        "wdio-chromedriver-service": "^7.2.2"
    }
